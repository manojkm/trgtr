                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="datetime-local" id="inputDateTime">
                                </div>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inputDate" class="col-2 col-form-label text-right">Date</label>
                            <div class="col-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="date" id="inputDate">
                                </div>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inputMonth" class="col-2 col-form-label text-right">Month</label>
                            <div class="col-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="month" id="inputMonth">
                                </div>
                            </div>
