                            <label for="inputPassword" class="col-sm-2 col-form-label text-right ">Password</label>
                            <div class="col-sm-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-lock"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="password" value="password" id="inputPassword">
                                </div>
                            </div>
                        </div>

