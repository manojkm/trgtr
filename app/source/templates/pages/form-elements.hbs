                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-phone"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="tel" value="1-(555)-555-5555" id="inputTelephone">
                                </div>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label for="inputDateTime" class="col-2 col-form-label text-right">Date and time</label>
