                                        <div class="input-group-text">
                                            <i class="icon-envelope"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="email" value="bootstrap@example.com"
                                           placeholder="Email" id="inputEmail">
                                </div>
                            </div>
