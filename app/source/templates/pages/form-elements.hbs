                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-envelope"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="email" value="bootstrap@example.com"
                                           placeholder="Email" id="inputEmail">
                                </div>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label for="inputURL" class="col-sm-2 col-form-label text-right ">URL</label>
                            <div class="col-sm-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-link"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="url" value="https://getbootstrap.com"
                                           placeholder="URL" id="inputURL">
                                </div>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label for="inputPassword" class="col-sm-2 col-form-label text-right ">Password</label>
                            <div class="col-sm-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-lock"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="password" value="password" id="inputPassword">
                                </div>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label for="inputNumber" class="col-sm-2 col-form-label text-right ">Number</label>
                            <div class="col-sm-10">
                                <input class="form-control" type="number" value="34" id="inputNumber">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label for="inputTelephone" class="col-sm-2 col-form-label text-right ">Telephone</label>
