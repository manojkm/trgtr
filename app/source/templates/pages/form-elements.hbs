                        <div class="form-group row">
                            <label for="inputURL" class="col-sm-2 col-form-label text-right ">URL</label>
                            <div class="col-sm-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-link"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="url" value="https://getbootstrap.com"
                                           placeholder="URL" id="inputURL">
                                </div>
                            </div>
                        </div>

